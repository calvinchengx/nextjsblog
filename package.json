{
  "name": "nextjsblog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "serve ..",
    "build": "next build && next export",
    "clean:project": "rimraf node_modules/.cache && rimraf out && rimraf _next",
    "clean:parent": "node tools/clean.js",
    "clean": "npm run clean:parent && npm run clean:project",
    "copy": "cp -rf out/* ../ && mkdir -p ../calvinchengx.github.io && cp -rf ../_next ../calvinchengx.github.io/",
    "deploy": "npm run clean && npm run build && npm run copy",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "gray-matter": "^4.0.3",
    "next": "^12.1.6",
    "node-glob": "^1.2.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.3",
    "tailwindcss": "^3.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.17.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-transform-stub": "^2.0.0",
    "react-test-renderer": "^18.1.0",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2"
  }
}
